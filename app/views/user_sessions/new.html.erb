 <br />
 <h1 align="center">Gerenciador de Finanças Pessoais</h1>
 <br />
 
 
 <div align="center"> 
 <%= form_for @user_session, :url => {:action => "create"} do |f| %>
    <%= f.error_messages %>
    <div>
      <%= f.label :login %>:
      <%= f.text_field :login %><br />
    </div>
    <div>
      <%= f.label :password %>:
      <%= f.password_field :password %><br />
    </div>
    <div>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
    <%= f.submit "Login"%>
  <% end %>
</div>