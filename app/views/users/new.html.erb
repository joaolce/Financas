<div align="center">
  <h1>Inclusão de usuário</h1>
</div>
<br />
 <div align="center">
  <%= form_for @user, :url => {:action => "create"} do |f| %>
    <%= f.error_messages %>
    <table>
      <tr>
        <td>
          <%= f.label :login %>:
        </td>
        <td>
          <%= f.text_field :login %>
        </td>
      </tr>
      <tr>
        <td>
          <%= f.label :email %>:
        </td>
        <td>
          <%= f.text_field :email %>
        </td>
      </tr>
      <tr>
        <td>
          <%= f.label :password %>:
        </td>
        <td>
          <%= f.password_field :password%>
        </td>
      </tr>
      <tr>
        <td>
          <%= f.label :password_confirmation%>:
        </td>
        <td>
          <%= f.password_field :password_confirmation %>
        </td>
      </tr>
      <tr>
        <td colspan="2">
          <div align="center">
            <%= f.submit "Salvar" %>
          </div>
        </td>
      </tr>
    </table>
  <% end %>
</div>